    <skos:narrower rdf:resource="http://os.uk/vocabularies/collection/Transport-Network" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/themes" />
    <skos:topConceptOf rdf:resource="http://os.uk/vocabularies/themes" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/theme/str">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Structures</skos:prefLabel>
