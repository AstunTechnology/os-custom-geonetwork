    <skos:definition xml:lang="en">The building theme contains buildings which meet the defintion of a permanent roofed construction, usually with walls.</skos:definition>
    <skos:narrower rdf:resource="http://os.uk/vocabularies/collection/Building-Features" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/themes" />
    <skos:topConceptOf rdf:resource="http://os.uk/vocabularies/themes" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/theme/trn">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Transport</skos:prefLabel>
    <skos:definition xml:lang="en">The transport theme contains both the geographical representation and routing network for Great Britains highway information.</skos:definition>
