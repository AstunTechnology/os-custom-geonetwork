    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Road</skos:prefLabel>
    <skos:definition xml:lang="en">A road with an accepted name or official government number.</skos:definition>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Transport-Network" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/tn" />
    <skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/featureconcept/Road" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/RoadJunction">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Road Junction</skos:prefLabel>
    <skos:definition xml:lang="en">An intersection of roads with an accepted name or official government number.</skos:definition>
    <skos:altLabel xml:lang="en">Road Junction</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Transport-Network" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/tn" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/RoadNode" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/RoadLine">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Road Line</skos:prefLabel>
    <skos:definition xml:lang="en">A linear feature associated with the passage of vehicles, pedestrians and animals being ridden or led.</skos:definition>
    <skos:altLabel xml:lang="en">Road Line</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Transport-Features" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/tn" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/RoadLinkSequence" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/RoadLink">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Road Link</skos:prefLabel>
    <skos:definition xml:lang="en">A network representation of the general alignment of a route used by vehicles and potentially other users.</skos:definition>
    <skos:altLabel xml:lang="en">Road Link</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Transport-Network" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/tn" />
    <skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/featureconcept/RoadLink" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/RoadNode">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Road Node</skos:prefLabel>
    <skos:definition xml:lang="en">The location where a road ends, is connected to two or more other paths or where a recorded attribute of a road changes.</skos:definition>
    <skos:altLabel xml:lang="en">Road Node</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Transport-Network" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/tn" />
    <skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/featureconcept/RoadNode" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/RoadTrackOrPath">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Road Track Or Path</skos:prefLabel>
    <skos:definition xml:lang="en">An area of Land associated with the passage of vehicles, pedestrians and animals being ridden or led.</skos:definition>
    <skos:altLabel xml:lang="en">Road Track Or Path</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Transport-Features" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/tn" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/RoadLinkSequence" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/RoutingHazard">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Routing Hazard</skos:prefLabel>
    <skos:definition xml:lang="en">A location where a threat is imposed and caution should be taken with relation to the highway network.</skos:definition>
    <skos:altLabel xml:lang="en">Routing Hazard</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/RAMI" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/tn" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/RoutingStructure">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Routing Structure</skos:prefLabel>
    <skos:definition xml:lang="en">A built feature with relation to the highway network.</skos:definition>
    <skos:altLabel xml:lang="en">Routing Structure</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/RAMI" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/tn" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/Site">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Site</skos:prefLabel>
    <skos:definition xml:lang="en">A multi-surface feature representing the extent of certain types of function or activity.</skos:definition>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Land-Use-Features" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/location" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/SiteAccessLocation">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Site Access Location</skos:prefLabel>
    <skos:definition xml:lang="en">A point feature representing the accepted and/or maintained locations where pedestrians and/or vehicles can enter or exit a site.</skos:definition>
    <skos:altLabel xml:lang="en">Site Access Location</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Land-Use-Features" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/SiteRoutingPoint">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Site Routing Point</skos:prefLabel>
    <skos:definition xml:lang="en">The location on the road network that provides entry or exit into a site for vehicles.</skos:definition>
    <skos:altLabel xml:lang="en">Site Routing Point</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Land-Use-Features" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/SpecialDesignationArea">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Special Designation Area</skos:prefLabel>
    <skos:definition xml:lang="en">A description applied to a highway to protect it during Street or Road Works, represented by an area feature.</skos:definition>
    <skos:altLabel xml:lang="en">Special Designation Area</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/RAMI" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/tn" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/SpecialDesignationLine">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Special Designation Line</skos:prefLabel>
    <skos:definition xml:lang="en">A description applied to a highway to protect it during Street or Road Works, represented by a line feature.</skos:definition>
    <skos:altLabel xml:lang="en">Special Designation Line</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/RAMI" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/tn" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/SpecialDesignationPoint">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Special Designation Point</skos:prefLabel>
    <skos:definition xml:lang="en">A description applied to a highway to protect it during Street or Road Works, represented by a point feature.</skos:definition>
    <skos:altLabel xml:lang="en">Special Designation Point</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/RAMI" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/tn" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/Street">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Street</skos:prefLabel>
    <skos:definition xml:lang="en">Any road, footway, path, cycletrack, track or passageway that has been uniquely identified by the local Highway Authority.</skos:definition>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Transport-Network" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/tn" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/Road" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/StreetAddress">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Street Address</skos:prefLabel>
    <skos:definition xml:lang="en">Any road, footway, path, cycletrack, track or passageway that has been uniquely identified by the local Highway Authority and has an addressable object.</skos:definition>
    <skos:altLabel xml:lang="en">Street Address</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/GB-Address" />
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Islands-Address" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/location" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/ad" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/ThoroughfareName" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/Structure">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Structure</skos:prefLabel>
    <skos:definition xml:lang="en">A manmade construction that is not a building.</skos:definition>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Structure-Features" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/structure" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/bu" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/StructureLine">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Structure Line</skos:prefLabel>
    <skos:definition xml:lang="en">A linear freestanding manmade construction that is not a building nor wide enough to capture as a Structure.</skos:definition>
    <skos:altLabel xml:lang="en">Structure Line</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Structure-Features" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/structure" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/bu" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/StructurePoint">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Structure Point</skos:prefLabel>
    <skos:definition xml:lang="en">A freestanding manmade construction that is not a building and that is less than 4m square but that is considered to be of sufficient interest to be captured.</skos:definition>
    <skos:altLabel xml:lang="en">Structure Point</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Structure-Features" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/structure" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/bu" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/TidalBoundary">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Tidal Boundary</skos:prefLabel>
    <skos:definition xml:lang="en">The alignment of the limit of tidal water at specific states of the tide.</skos:definition>
    <skos:altLabel xml:lang="en">Tidal Boundary</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Water-Features" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/oceans" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/hy" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/LandWaterBoundary" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/UpperTierLocalAuthority">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Upper Tier Local Authority</skos:prefLabel>
    <skos:definition xml:lang="en">The upper tier of local government. Includes unitary authorities, non-metropolitan counties, metropolitan districts and London boroughs.</skos:definition>
    <skos:altLabel xml:lang="en">Upper Tier Local Authority</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/au" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/AdministrativeUnit" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/Ward">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Ward</skos:prefLabel>
    <skos:definition xml:lang="en">Electoral wards/divisions are the key building blocks of UK administrative geography. They are the spatial units used to elect local government councilors in metropolitan and non-metropolitan districts, unitary authorities and the London boroughs in England; unitary authorities in Wales and council areas in Scotland. Electoral wards are specifically used in communities, districts, London boroughs, metropolitan districts, Scottish unitary authorities and some English unitary authorities.</skos:definition>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/au" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/AdministrativeUnit" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/Water">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Water</skos:prefLabel>
    <skos:definition xml:lang="en">An area of permanent water.</skos:definition>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Water-Features" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/inlandWaters" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/hy" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/StandingWater" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/WaterLink">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Water Link</skos:prefLabel>
    <skos:definition xml:lang="en">A network representation of the general alignment of areas of permanent water where they are connected.</skos:definition>
    <skos:altLabel xml:lang="en">Water Link</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Water-Network" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/inlandWaters" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/hy" />
    <skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/featureconcept/WatercourseLink" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/WaterLinkSet">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Water Link Set</skos:prefLabel>
    <skos:definition xml:lang="en">A body of water with an accepted name that the water network flows through.</skos:definition>
    <skos:altLabel xml:lang="en">Water Link Set</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Water-Network" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/inlandWaters" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/hy" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/Watercourse" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/WaterNode">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Water Node</skos:prefLabel>
    <skos:definition xml:lang="en">The location where a connected area of permanent water ends, is connected to two or more other areas of permanent water or where a recorded attribute changes.</skos:definition>
    <skos:altLabel xml:lang="en">Water Node</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Water-Network" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/inlandWaters" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/hy" />
    <skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/featureconcept/HydroNode" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/WaterPoint">
