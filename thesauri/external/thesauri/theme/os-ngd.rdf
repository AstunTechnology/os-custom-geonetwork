    <skos:definition xml:lang="en">A network representation of the general alignment of a route used by pedestrians.</skos:definition>
    <skos:altLabel xml:lang="en">Path Link</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Transport-Network" />
