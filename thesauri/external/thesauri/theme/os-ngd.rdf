    <skos:inScheme rdf:resource="http://os.uk/vocabularies/themes" />
    <skos:topConceptOf rdf:resource="http://os.uk/vocabularies/themes" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/theme/wtr">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Water</skos:prefLabel>
    <skos:definition xml:lang="en">The water theme contains features which are defined as features that contain, delimit, or relate to real-world objects containing water; as well as a three-dimensional structured link and node network of watercourses for Great Britain.</skos:definition>
