    <skos:prefLabel xml:lang="en">Pre Build Address</skos:prefLabel>
    <skos:definition xml:lang="en">A new address for a plot of land, a building or part of a building, or some other construction that can conceivably receive mail, deliveries or services but has yet to be built or is under construction.</skos:definition>
    <skos:altLabel xml:lang="en">Pre Build Address</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/GB-Address" />
