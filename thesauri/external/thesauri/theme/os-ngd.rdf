  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/NamedArea">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Named Area</skos:prefLabel>
    <skos:definition xml:lang="en">An area of land or water that has a name, that could not be captured in any of the other data themes.</skos:definition>
