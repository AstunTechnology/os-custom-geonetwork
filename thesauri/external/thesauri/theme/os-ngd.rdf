  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/NonAddressableObject">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Non Addressable Object</skos:prefLabel>
    <skos:definition xml:lang="en">An address for a plot of land, a building or part of a building, or some other construction that cannot conceivably receive mail, deliveries or services.</skos:definition>
