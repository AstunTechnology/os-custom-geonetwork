  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">National Geographic Database</skos:prefLabel>
    <skos:definition xml:lang="en">The National Geographic Database (NGD) contains the authoritative data that describes the geography of Great Britain</skos:definition>
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/themes" />
    <skos:topConceptOf rdf:resource="http://os.uk/vocabularies/themes" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/theme/add">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Address</skos:prefLabel>
    <skos:definition xml:lang="en">The address theme contains point locations for addresses within GB and beyond</skos:definition>
    <skos:narrower rdf:resource="http://os.uk/vocabularies/collection/GB-Address" />
    <skos:narrower rdf:resource="http://os.uk/vocabularies/collection/Islands-Address" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/themes" />
    <skos:topConceptOf rdf:resource="http://os.uk/vocabularies/themes" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/theme/asu">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Administrative And Statistical Units</skos:prefLabel>
    <skos:definition xml:lang="en">The boundary theme contains data which provides a representation of the hierarchy of administrative and electoral boundaries and their names for England, Scotland and Wales</skos:definition>
    <skos:narrower rdf:resource="http://os.uk/vocabularies/collection/Boundaries" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/themes" />
    <skos:topConceptOf rdf:resource="http://os.uk/vocabularies/themes" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/theme/bld">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
