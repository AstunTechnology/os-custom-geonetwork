  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/MaintenanceArea">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Maintenance Area</skos:prefLabel>
    <skos:definition xml:lang="en">Indication of whether the highway is maintained by a Highways Authority, Local Highways Authority, Road Authority or privately, represented by an area feature.</skos:definition>
    <skos:altLabel xml:lang="en">Maintenance Area</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/RAMI" />
