    <skos:definition xml:lang="en">The standard to which the highway must be restored to following street works, represented by an area feature.</skos:definition>
    <skos:altLabel xml:lang="en">Reinstatement Area</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/RAMI" />
