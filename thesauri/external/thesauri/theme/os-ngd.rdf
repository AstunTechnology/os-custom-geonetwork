  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme" />
    <dct:title xml:lang="en">Ordnance Survey NGD vocabulary</dct:title>
    <dct:description xml:lang="en">Pre-release trial SKOS version of evolving NGD vocab, 2022-02-15 extract from NGD_Metadata_And_Definitions_MASTER</dct:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/BoundaryHighWaterMark">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Boundary High Water Mark</skos:prefLabel>
    <skos:definition xml:lang="en">The alignment of the Mean High Water mark in England &amp; Wales or the Mean High Water Springs mark in Scotland, at a level of detail consistent with the boundary polygons. In places where there are vertical cliffs and walls, the line is coincident with the boundary polygon perimeter, otherwise it will be inside, enclosing the intertidal area.</skos:definition>
    <skos:altLabel xml:lang="en">Boundary High Water Mark</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/au" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/AdministrativeBoundary" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/BuildingLine">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Building Line</skos:prefLabel>
    <skos:definition xml:lang="en">Either a wall between two buildings or the external alignment of an overhead building or canopy.</skos:definition>
    <skos:altLabel xml:lang="en">Building Line</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Building-Features" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/structure" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/bu" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/Building" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/BuildingPart">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Building Part</skos:prefLabel>
    <skos:definition xml:lang="en">Either a complete roofed structure or part of a larger roofed structure where internal divisions exist from ground to roof level and they can be identified externally.</skos:definition>
    <skos:altLabel xml:lang="en">Building Part</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Building-Features" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/structure" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/bu" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/BuildingPart" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/BuiltAddress">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Built Address</skos:prefLabel>
    <skos:definition xml:lang="en">An active address for a building or similar construction that can conceivably receive mail, deliveries or services.</skos:definition>
    <skos:altLabel xml:lang="en">Built Address</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/GB-Address" />
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Islands-Address" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/location" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/ad" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/Address" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/CartographicRailDetail">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Cartographic Rail Detail</skos:prefLabel>
    <skos:definition xml:lang="en">A continuous line of steel bars that as a set of parallel features guide the movement of a rail vehicle wheels.</skos:definition>
    <skos:altLabel xml:lang="en">Cartographic Rail Detail</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Transport-Features" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/tn" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/CartographicRoadTrackOrPathLine">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Cartographic Road Track Or Path Line</skos:prefLabel>
    <skos:definition xml:lang="en">A manmade linear feature associated with associated with the passage of vehicles, pedestrians and animals being ridden or led not wide enough to capture as a RoadTrackOrPath.</skos:definition>
    <skos:altLabel xml:lang="en">Cartographic Road Track or Path Line</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Transport-Features" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/tn" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/Road" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/CeremonialCounty">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Ceremonial County</skos:prefLabel>
    <skos:definition xml:lang="en">An area that has a representative appointed by the monarch as a personal representative (Lord Lieutenant) or has a High Sheriff.</skos:definition>
    <skos:altLabel xml:lang="en">Ceremonial County</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/au" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/AdministrativeUnit" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/CompoundStructure">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Compound Structure</skos:prefLabel>
    <skos:definition xml:lang="en">A manmade construction built for a specific purpose.</skos:definition>
    <skos:altLabel xml:lang="en">Compound Structure</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Structure-Features" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/structure" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/bu" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/ConnectingLink">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Connecting Link</skos:prefLabel>
    <skos:definition xml:lang="en">An indicative connection between the Path network and the Road network that may not reflect any specific real world feature or alignment.</skos:definition>
    <skos:altLabel xml:lang="en">Connecting Link</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Transport-Network" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/tn" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/RoadLink" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/ConnectingNode">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Connecting Node</skos:prefLabel>
    <skos:definition xml:lang="en">The location on the road network that indicates a connection to the Path Network.</skos:definition>
    <skos:altLabel xml:lang="en">Connecting Node</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Transport-Network" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/tn" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/RoadNode" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/Country">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Country</skos:prefLabel>
    <skos:definition xml:lang="en">A country is a distinct territorial body or political entity (i.e. a nation). Currenly includes England, Wales and Scotland.</skos:definition>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/au" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/AdministrativeUnit" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/DevolvedParliamentConstituency">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Devolved Parliament Constituency</skos:prefLabel>
    <skos:definition xml:lang="en">A parliamentary constituency in Scotland or Wales but not in England.</skos:definition>
    <skos:altLabel xml:lang="en">Devolved Parliament Constituency</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/au" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/AdministrativeUnit" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/DevolvedParliamentElectoralRegion">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Devolved Parliament Electoral Region</skos:prefLabel>
    <skos:definition xml:lang="en">An electoral region in Scotland or Wales but not in England.</skos:definition>
    <skos:altLabel xml:lang="en">Devolved Parliament Electoral Region</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/au" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/AdministrativeUnit" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/ElectoralDivision">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Electoral Division</skos:prefLabel>
    <skos:definition xml:lang="en">Electoral wards/divisions are the key building blocks of UK administrative geography. They are the spatial units used to elect local government councilors in metropolitan and non-metropolitan districts, unitary authorities and the London boroughs in England; unitary authorities in Wales and council areas in Scotland. Electoral divisions are specifically used in non-metropolitan counties, Welsh unitary authorities and some English unitary authorities.</skos:definition>
    <skos:altLabel xml:lang="en">Electoral Division</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/au" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/AdministrativeUnit" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/FerryLink">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Ferry Link</skos:prefLabel>
    <skos:definition xml:lang="en">A scheduled service between two ferry terminals for vehicles and or pedestrians.</skos:definition>
    <skos:altLabel xml:lang="en">Ferry Link</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Transport-Network" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/tn" />
    <skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/featureconcept/FerryCrossing" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/FerryNode">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Ferry Node</skos:prefLabel>
    <skos:definition xml:lang="en">The location at the end of the Ferry network where the user transitions to road or path.</skos:definition>
    <skos:altLabel xml:lang="en">Ferry Node</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Transport-Network" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/tn" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/PortNode" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/FerryTerminal">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Ferry Terminal</skos:prefLabel>
    <skos:definition xml:lang="en">An indicative connection between the Ferry Network and the Path or Road network.</skos:definition>
    <skos:altLabel xml:lang="en">Ferry Terminal</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Transport-Network" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/tn" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/PortNode" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/GLAAssemblyConstituency">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">GLA Assembly Constituency</skos:prefLabel>
    <skos:definition xml:lang="en">Greater London Authority (GLA) Assembly constituencies are used to elect members to the London Assembly, which is part of the GLA.</skos:definition>
    <skos:altLabel xml:lang="en">Greater London Authority Assembly Constituency</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/au" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/AdministrativeUnit" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/HighwayDedication">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Highway Dedication</skos:prefLabel>
    <skos:definition xml:lang="en">Definition of which type of Highway user has access to a particular section of the Highway.</skos:definition>
    <skos:altLabel xml:lang="en">Highway Dedication</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/RAMI" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/tn" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/RestrictionForVehicles" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/HistoricAddress">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Historic Address</skos:prefLabel>
    <skos:definition xml:lang="en">A former address for a building or similar construction that could conceivably receive mail, deliveries or services prior to becoming historic.</skos:definition>
    <skos:altLabel xml:lang="en">Historic Address</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/GB-Address" />
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Islands-Address" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/location" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/ad" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/Address" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/HistoricCounty">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Historic County</skos:prefLabel>
    <skos:definition xml:lang="en">Counties based on historic records and mapping circa 1888 and using the primary sources of the Local Government (England and Wales) Act 1888, the Local Government (Scotland) Act 1889 and the Sheriffs Act 1887. The historic counties dataset shows the county boundaries in place in 1888 in England and Wales and those established a year later in Scotland. The alignments of the counties for England and Wales are derived from mapping sourced from the National Archives and dating from 1890. The most recent available mapping dates for Scotland are from 1940.</skos:definition>
    <skos:altLabel xml:lang="en">Historic County</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/au" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/AdministrativeUnit" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/HistoricEuropeanRegion">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Historic European Region</skos:prefLabel>
    <skos:definition xml:lang="en">The electoral regions used for elections to the European Parliament when the UK was a member of the European Union.</skos:definition>
    <skos:altLabel xml:lang="en">Historic European Region</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/boundaries" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/au" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/AdministrativeUnit" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/InterTidalLine">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Inter Tidal Line</skos:prefLabel>
    <skos:definition xml:lang="en">A watercourse extending across the intertidal zone.</skos:definition>
    <skos:altLabel xml:lang="en">Inter Tidal Line</skos:altLabel>
