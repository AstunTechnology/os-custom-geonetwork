    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/MaintenancePoint">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Maintenance Point</skos:prefLabel>
    <skos:definition xml:lang="en">Indication of whether the highway is maintained by a Highways Authority, Local Highways Authority, Road Authority or privately, represented by a point feature.</skos:definition>
    <skos:altLabel xml:lang="en">Maintenance Point</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/RAMI" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/transportation" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/tn" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/MaintenanceAuthority" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
