    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/Land">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Land</skos:prefLabel>
    <skos:definition xml:lang="en">An area on the earth's surface that has not otherwise been captured as either a BuildingPart, Rail, RoadTrackOrPath, Structure or Water feature.</skos:definition>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Land-Features" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/imageryBaseMapsEarthCover" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/theme/lc" />
    <skos:broader rdf:resource="http://inspire.ec.europa.eu/featureconcept/LandCoverUnit" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/Landform">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Landform</skos:prefLabel>
    <skos:definition xml:lang="en">An area of the earth's surface with a distinct physical morphology such as cliffs, artificial slopes, ravines and some former mineral workings.</skos:definition>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Land-Features" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/environment" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/LandformLine">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Landform Line</skos:prefLabel>
    <skos:definition xml:lang="en">The alignment of either the higher limit or the side or lower limit of areas with a distinct physical morphology, typically where there is a signficant change in slope angle.</skos:definition>
    <skos:altLabel xml:lang="en">Landform Line</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Land-Features" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/environment" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/LandformPoint">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Landform Point</skos:prefLabel>
    <skos:definition xml:lang="en">An area of the earth's surface with a distinct physical morphology that is too small to capture as a Landform feature but that is considered to be of sufficient interest to be captured, typically associated with former mineral workings.</skos:definition>
    <skos:altLabel xml:lang="en">Landform Point</skos:altLabel>
    <skos:broader rdf:resource="http://os.uk/vocabularies/collection/Land-Features" />
    <skos:broader rdf:resource="https://inspire.ec.europa.eu/metadata-codelist/TopicCategory/environment" />
    <skos:inScheme rdf:resource="http://os.uk/vocabularies/ngd" />
  </rdf:Description>
  <rdf:Description rdf:about="http://os.uk/vocabularies/ngd/LowerTierLocalAuthority">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Lower Tier Local Authority</skos:prefLabel>
    <skos:definition xml:lang="en">The lower tier of local government. Includes unitary authorities, districts, metropolitan districts and London boroughs.</skos:definition>
